<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.clock"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/pobrane"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="org.clock.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="org.clock.ShowAlarmsActivity" >
        </activity>
            
        <activity android:name="org.timepicker.MainActivity" >
        </activity>
        
        <activity android:name="org.timepicker.PeriodActivity" >
        </activity>
        <service android:enabled="true" android:name=".MyService" />
    </application>

</manifest>